    },
    "dependencies": {
        "date-fns-tz": "^3.2.0",
        "marked": "^11.1.1"
    },
    "devDependencies": {
        "jest": "^30.2.0",
        "jest-environment-jsdom": "^30.2.0",
        "babel-jest": "^30.2.0"
    }
